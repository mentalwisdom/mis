@model IEnumerable<MIS.Models.ProductGroup>

@{ int count =0;}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb breadcrumb-arrow">
        <li class="breadcrumb-item active" aria-current="page">ProductGroup</li>
    </ol>
</nav>
<hr>    
<a asp-action="Create" class="btn button btn-outline-success">
    <i class="fa fa-plus"></i>
        Create
</a>
<br>
<br>
<table class="table">
    <thead>
        <tr>
            <th>
                
                product group name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.product_group_name
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.product_group_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.product_group_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.product_group_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
