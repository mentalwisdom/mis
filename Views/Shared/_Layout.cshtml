<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - fp</title>
    <link rel="stylesheet" href="~/css/site.css" />
</head>

<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light header-fp border-bottom box-shadow mb-3 fixed-top">
              <a class="navbar-brand d-inline-block align-top" 
                
                    asp-area="" asp-controller="Home" asp-action="Index">
                        <h3>MIS </h3>
                  </a>
  
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/admin">business process</a>
                        </li>
                        <li class="nav-item text-white">
                            <partial name="_BusinessEntityPartial" />
                        </li>
                    </ul>
                </div>
            
        </nav>
    </header>
    <div class="container-fluid">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3">
            <br>
            <br>
            <br>
            <br>
            <br>
            @RenderBody()
        </main>
    </div>

